{
  "name": "kebab-api",
  "version": "1.0.0",
  "description": "Backend API for [kebab](https://github.com/wAndrewx/kebab-ui)",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "dev": "nodemon ./index.js",
