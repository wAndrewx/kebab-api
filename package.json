    "build:dev": "serverless deploy --stage dev",
    "build:prod": "serverless deploy --stage production"
  },
  "author": "Andrew Huynh | @wAndrewx",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "nodemailer": "^6.6.3",
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.5"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules"
    ]
